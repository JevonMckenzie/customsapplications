<p id="notice"><%= notice %></p>

<h1>Refunds</h1>

<table>
  <thead>
    <tr>
      <th>Vessel</th>
      <th>Rotnum</th>
      <th>Entrynum</th>
      <th>Refundof</th>
      <th>Enteredas</th>
      <th>Enteredasq</th>
      <th>Enteredasv</th>
      <th>Enteredasr</th>
      <th>Enteredasd</th>
      <th>Shouldbe</th>
      <th>Shouldbeq</th>
      <th>Shouldbev</th>
      <th>Shouldber</th>
      <th>Shouldbed</th>
      <th>Reasonsupportrefund</th>
      <th>Refundclaimed</th>
      <th>Date</th>
      <th>Signature</th>
      <th>Appdate</th>
      <th>Compsig</th>
      <th>Claimnum</th>
      <th>Receiveddate</th>
      <th>Claimantsig</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @refunds.each do |refund| %>
      <tr>
        <td><%= refund.vessel %></td>
        <td><%= refund.rotNum %></td>
        <td><%= refund.entryNum %></td>
        <td><%= refund.refundof %></td>
        <td><%= refund.enteredAs %></td>
        <td><%= refund.enteredAsQ %></td>
        <td><%= refund.enteredAsV %></td>
        <td><%= refund.enteredAsR %></td>
        <td><%= refund.enteredAsD %></td>
        <td><%= refund.shouldBe %></td>
        <td><%= refund.shouldBeQ %></td>
        <td><%= refund.shouldBeV %></td>
        <td><%= refund.shouldBeR %></td>
        <td><%= refund.shouldBeD %></td>
        <td><%= refund.reasonSupportRefund %></td>
        <td><%= refund.refundClaimed %></td>
        <td><%= refund.date %></td>
        <td><%= refund.signature %></td>
        <td><%= refund.appDate %></td>
        <td><%= refund.compSig %></td>
        <td><%= refund.claimNum %></td>
        <td><%= refund.receivedDate %></td>
        <td><%= refund.claimantSig %></td>
        <td><%= refund.user_id %></td>
        <td><%= link_to 'Show', refund %></td>
        <td><%= link_to 'Edit', edit_refund_path(refund) %></td>
        <td><%= link_to 'Destroy', refund, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Refund', new_refund_path %>
